<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/transparent"
    >

    <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="1dp"-->
        <!--android:background="@color/line_view"-->
        <!--&gt;-->

    <!--</View>-->


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:background="@drawable/dialog_backgroud"
       >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10">


                <TextView
                    android:id="@+id/id_bike_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/x15_dp"
                    android:text="单车编号："
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_text_main" />

                <TextView
                    android:id="@+id/tv_car_id_order"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/id_bike_number"
                    android:text="33500000001"
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_text_main" />

            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10">

                <TextView
                    android:id="@+id/id_bike_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/x15_dp"
                    android:text="开始时间："
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_dp" />

                <TextView
                    android:id="@+id/tv_car_start"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/id_bike_tv"
                    android:text="12：00：00"
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_text_main" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/x15_dp"
                android:layout_marginTop="@dimen/x10">

                <TextView
                    android:id="@+id/id_bike_money_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/x15_dp"
                    android:text="预计花费："
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_dp" />

                <TextView
                    android:id="@+id/tv_money_order"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/id_bike_money_tv"
                    android:layout_toRightOf="@id/id_bike_money_tv"
                    android:text="0.50元"
                    android:textColor="@color/x3"
                    android:textSize="@dimen/x15_text_main" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:textColor="@color/viewfinder_laser"
                    android:textSize="@dimen/app_text_content"
                    android:gravity="center_horizontal"
                    android:text="@string/tishi_tips"
                    android:background="@color/white"
                    android:layout_below="@+id/tv_money_order"
                    android:layout_marginTop="@dimen/x10"
                    />
            </RelativeLayout>



        </LinearLayout>

        <TextView
            android:id="@+id/tv_return_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="70dp"
            android:layout_marginRight="@dimen/x20_main"
            android:layout_marginTop="15dp"
            android:background="@mipmap/order_return"
            android:gravity="center"
             android:text="结束用车"
            android:textColor="@color/white"
            android:textSize="@dimen/app_text_content"
            />

    </RelativeLayout>


    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/x20_main"
        android:layout_marginTop="@dimen/x15_dp"
        android:background="@mipmap/btn11"
        android:text="结束还车"
        android:gravity="center"
        android:visibility="gone"
        android:textColor="@color/white"
        android:textSize="@dimen/app_text_content" />



    <LinearLayout
        android:id="@+id/ll_near_pass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/transparent"
        android:padding="10dp"
        android:gravity="center_horizontal"

        >
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/near_stop"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="附近停车位"
        android:textColor="@color/contents_text"
        android:textSize="8sp"
        />
    </LinearLayout>
</LinearLayout>
