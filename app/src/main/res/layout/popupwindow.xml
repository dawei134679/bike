<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/money_backgroud"
    >


    <Button
        android:id="@+id/id_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="会员"
        android:visibility="gone"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000"
        android:visibility="gone"
        >

    </View>
    <Button
        android:id="@+id/id_no_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="非会员"
        android:visibility="gone"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="订单信息"
        android:textColor="@color/monet_card_text"
        android:textSize="@dimen/app_text_content"
        android:layout_gravity="center"

        />

    <TextView
        android:id="@+id/tv_return_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/btn11"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/x20_main"
        android:layout_marginTop="@dimen/x30"
        android:text="结束用车"
        android:textColor="@color/white"
        android:textSize="@dimen/app_text_content" />
</LinearLayout>
